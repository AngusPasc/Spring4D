  Spring.Logging.Repositories in '..\Source\Core\Logging\Spring.Logging.Repositories.pas',
  Spring.Logging.ResourceStrings in '..\Source\Core\Logging\Spring.Logging.ResourceStrings.pas',
  Spring.Configuration in '..\Source\Core\Configuration\Spring.Configuration.pas',
  Spring.Configuration.Xml in '..\Source\Core\Configuration\Spring.Configuration.Xml.pas',
  Spring.DI.Builder in '..\Source\Core\DI\Spring.DI.Builder.pas',
