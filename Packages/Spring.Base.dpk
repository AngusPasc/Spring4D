/// <summary>
/// Contains the <b>Base Class Library</b> of Spring Framework for Delphi.
/// </summary>
package Spring.Base;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Spring Framework for Delphi Base Class Library'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  rtl,
  vcl,
  dbrtl;

contains
  Spring.DesignPatterns in '..\Source\Base\Spring.DesignPatterns.pas',
  Spring.ResourceStrings in '..\Source\Base\Spring.ResourceStrings.pas',
  Spring.Reflection in '..\Source\Base\Spring.Reflection.pas',
  Spring.Helpers in '..\Source\Base\Spring.Helpers.pas',
  Spring.Collections.Adapters in '..\Source\Base\Collections\Spring.Collections.Adapters.pas',
  Spring.Collections.Extensions in '..\Source\Base\Collections\Spring.Collections.Extensions.pas',
  Spring.Collections in '..\Source\Base\Collections\Spring.Collections.pas',
  Spring.Utils.IO in '..\Source\Base\Utils\Spring.Utils.IO.pas',
  Spring.Utils in '..\Source\Base\Utils\Spring.Utils.pas',
  Spring.Utils.Network in '..\Source\Base\Utils\Spring.Utils.Network.pas',
  Spring.Pool in '..\Source\Base\Spring.Pool.pas',
  Spring.Cryptography.CRC in '..\Source\Base\Cryptography\Spring.Cryptography.CRC.pas',
  Spring.Cryptography.DES in '..\Source\Base\Cryptography\Spring.Cryptography.DES.pas',
  Spring.Cryptography.MD5 in '..\Source\Base\Cryptography\Spring.Cryptography.MD5.pas',
  Spring.Cryptography in '..\Source\Base\Cryptography\Spring.Cryptography.pas',
  Spring.Cryptography.SHA in '..\Source\Base\Cryptography\Spring.Cryptography.SHA.pas',
  Spring.Cryptography.Utils in '..\Source\Base\Cryptography\Spring.Cryptography.Utils.pas',
  Spring in '..\Source\Base\Spring.pas',
  Spring.ValueConverters in '..\Source\Base\Spring.ValueConverters.pas',
  Spring.Utils.Win32API in '..\Source\Base\Utils\Spring.Utils.Win32API.pas';

end.

