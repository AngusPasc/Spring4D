package Spring.Base;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Delphi Spring Framework Base Class Library'}
{$IMPLICITBUILD ON}
{$DEFINE DEBUG}

requires
  rtl,
  vcl,
  dbrtl;

contains
  Spring.DesignPatterns in 'Source\Base\Spring.DesignPatterns.pas',
  Spring.ResourceStrings in 'Source\Base\Spring.ResourceStrings.pas',
  Spring.System in 'Source\Base\Spring.System.pas',
  Spring.Utils in 'Source\Base\Spring.Utils.pas',
  Spring.Win32API in 'Source\Base\Spring.Win32API.pas',
  Spring.Reflection in 'Source\Base\Spring.Reflection.pas',
  Spring.Helpers in 'Source\Base\Spring.Helpers.pas',
  Spring.Collections.Adapters in 'Source\Base\Collections\Spring.Collections.Adapters.pas',
  Spring.Collections.Extensions in 'Source\Base\Collections\Spring.Collections.Extensions.pas',
  Spring.Collections in 'Source\Base\Collections\Spring.Collections.pas';

end.
